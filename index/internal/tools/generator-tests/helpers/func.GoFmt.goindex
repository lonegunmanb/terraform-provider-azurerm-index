package github.com/hashicorp/terraform-provider-azurerm/internal/tools/generator-tests/helpers
import (
	"errors"
	"fmt"
	"os"
	"os/exec"
)
func GoFmt(file string) error {
	cmd := exec.Command("gofmt", "-w", fmt.Sprintf("./%s", file))
	if _, err := cmd.Output(); err != nil {
		return err
	}

	return nil
}
