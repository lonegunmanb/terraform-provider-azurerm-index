package github.com/hashicorp/terraform-provider-azurerm/internal/tools/document-lint/model
import (
	"log"
	"strconv"
	"strings"

	"github.com/hashicorp/terraform-provider-azurerm/internal/tools/document-lint/util"
)
func (p Properties) Merge(p1 Properties) {
	if p == nil {
		return
	}
	for k, v := range p1 {
		// if exists already, it maybe should be in attr
		if f, ok := p[k]; ok {
			f.SameNameAttr = v
		} else {
			p[k] = v
		}
	}
}
