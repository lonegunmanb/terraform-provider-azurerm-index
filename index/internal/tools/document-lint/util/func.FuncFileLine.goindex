package github.com/hashicorp/terraform-provider-azurerm/internal/tools/document-lint/util
import (
	"reflect"
	"runtime"
)
func FuncFileLine(f interface{}) (file string, line int) {
	vf, ok := f.(reflect.Value)
	if !ok {
		vf = reflect.ValueOf(f)
	}
	if vf.IsNil() {
		return
	}
	return PointerFileLine(vf)
}
