package github.com/hashicorp/terraform-provider-azurerm/internal/tools/document-lint/md
import (
	"os"
	"regexp"
	"strings"
	"unicode"

	"github.com/hashicorp/terraform-provider-azurerm/internal/tools/document-lint/model"
)
func requireIndex(line string) int {
	if idx := strings.Index(line, "(Optional)"); idx > 0 {
		return idx
	}

	if idx := strings.Index(line, "(Required)"); idx > 0 {
		return idx
	}
	return -1
}
