func newPossibleValueDiff(checkBase checkBase, want []string, got []string, missed []string, spare []string) *possibleValueDiff {
	return &possibleValueDiff{
		checkBase: checkBase,
		Want:      want,
		Got:       got,
		Missed:    missed,
