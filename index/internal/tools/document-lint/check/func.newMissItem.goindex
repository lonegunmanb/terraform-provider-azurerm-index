package github.com/hashicorp/terraform-provider-azurerm/internal/tools/document-lint/check
import (
	"fmt"
	"strings"

	"github.com/hashicorp/terraform-provider-azurerm/internal/tools/document-lint/model"
	"github.com/hashicorp/terraform-provider-azurerm/internal/tools/document-lint/util"
)
func newMissItem(path string, f *model.Field, typ MissType) Checker {
	base := newCheckBase(0, path, f)
	if f != nil {
		base.line = f.Line
	}
	return newPropertyMiss(base, typ)
}
