package github.com/hashicorp/terraform-provider-azurerm/internal/tools/document-lint/check
import (
	"fmt"
	"strings"

	"github.com/hashicorp/terraform-provider-azurerm/internal/tools/document-lint/md"
	"github.com/hashicorp/terraform-provider-azurerm/internal/tools/document-lint/util"
)
func (c defaultDiff) String() string {
	if c.Default == "" {
		return fmt.Sprintf("%s should %s have default value", c.Str(), util.Bold("NOT"))
	}
	return fmt.Sprintf("%s default value should be `%s`", c.Str(), c.Default)
}
