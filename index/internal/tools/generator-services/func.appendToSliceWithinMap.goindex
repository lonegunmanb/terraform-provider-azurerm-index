func appendToSliceWithinMap(sliceMap map[string][]string, slice []string, key string) map[string][]string {
	if _, ok := sliceMap[key]; ok {
		sliceMap[key] = append(slice, sliceMap[key]...)
	} else {
		sliceMap[key] = slice
	}
	return sliceMap
}
