package github.com/hashicorp/terraform-provider-azurerm/internal/tools/templatehelpers
import (
	"errors"
	"fmt"
	"os"
	"os/exec"
)
func GoImports(file string) error {
	cmd := exec.Command("goimports", "-w", fmt.Sprintf("./%s", file))

	if _, err := cmd.CombinedOutput(); err != nil {
		return err
	}

	return nil
}
