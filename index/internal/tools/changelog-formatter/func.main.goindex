package github.com/hashicorp/terraform-provider-azurerm/internal/tools/main
import (
	"fmt"
	"os"
	"regexp"
	"slices"
	"strings"
)
func main() {
	if len(os.Args) != 2 {
		fmt.Println("Usage: go run main.go <path to changelog>")
		return
	}
	filePath := os.Args[1]

	if err := formatChangelog(filePath); err != nil {
		fmt.Println(fmt.Errorf("formatting changelog: %+v", err))
		return
	}
}
