package github.com/hashicorp/terraform-provider-azurerm/internal/tools/document-fmt/data
import (
	"fmt"
	"strings"
)
func (t Timeout) duration() string {
	duration := t.Duration
	unit := "minute"

	if (duration % 60) == 0 {
		duration /= 60
		unit = "hour"
	}

	if duration > 1 {
		unit += "s"
	}

	return fmt.Sprintf("%d %s", duration, unit)
}
