package github.com/hashicorp/terraform-provider-azurerm/internal/tools/document-fmt/rule
import (
	"fmt"
	"regexp"
	"strings"

	"github.com/hashicorp/terraform-provider-azurerm/internal/tools/document-fmt/data"
	"github.com/hashicorp/terraform-provider-azurerm/internal/tools/document-fmt/differror"
)
func normalizeNoteText(text string) string {
	// may have to expand on this regex in the future
	regex := regexp.MustCompile(`(?i)(^(note)?(:)?)`)

	return strings.TrimSpace(regex.ReplaceAllString(strings.TrimSpace(text), ""))
}
