package github.com/hashicorp/terraform-provider-azurerm/internal/tools/document-fmt/util
import (
	"github.com/spf13/afero"
)
func Map2Slices[K comparable, V any](m map[K]V) ([]K, []V) {
	keys, values := make([]K, 0, len(m)), make([]V, 0, len(m))
	for k, v := range m {
		keys = append(keys, k)
		values = append(values, v)
	}

	return keys, values
}
