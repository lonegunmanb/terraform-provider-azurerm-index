package github.com/hashicorp/terraform-provider-azurerm/internal/sdk
import (
	"errors"
	"fmt"
	"strconv"
	"strings"

	"github.com/hashicorp/go-cty/cty"
)
func (rmd ResourceMetaData) IsNullAt(key string) bool {
	val, err := rmd.GetRawConfigAt(key)
	if err != nil {
		return false
	}

	return val.IsNull()
}
