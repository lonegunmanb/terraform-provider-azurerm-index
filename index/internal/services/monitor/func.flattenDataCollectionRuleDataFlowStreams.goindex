func flattenDataCollectionRuleDataFlowStreams(input *[]datacollectionrules.KnownDataFlowStreams) []string {
	if input == nil {
		return make([]string, 0)
	}

	result := make([]string, 0)
	for _, v := range *input {
		result = append(result, string(v))
	}
	return result
}
