func expandDataCollectionEndpointKind(input string) *datacollectionendpoints.KnownDataCollectionEndpointResourceKind {
	if input == "" {
		return nil
	}

	result := datacollectionendpoints.KnownDataCollectionEndpointResourceKind(input)
	return &result
}
