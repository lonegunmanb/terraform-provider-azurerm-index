func flattenAlertProcessingRuleRecurrenceDaysOfWeek(input *[]alertprocessingrules.DaysOfWeek) []string {
	if input == nil {
		return make([]string, 0)
	}

	result := make([]string, 0)
	for _, item := range *input {
		result = append(result, string(item))
	}

	return result
}
