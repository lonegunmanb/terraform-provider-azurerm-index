package github.com/hashicorp/terraform-provider-azurerm/internal/services/monitor/validate
import (
	"regexp"

	"github.com/hashicorp/terraform-provider-azurerm/internal/tf/pluginsdk"
	"github.com/hashicorp/terraform-provider-azurerm/internal/tf/validation"
)
func AlertProcessingRuleScheduleDayTime() pluginsdk.SchemaValidateFunc {
	// from https://github.com/Azure/azure-rest-api-specs/issues/19141
	return validation.StringMatch(regexp.MustCompile(`^\d{2}:\d{2}:\d{2}$`),
		"expected date time format is: 'HH':'mm':'ss'.")
}
