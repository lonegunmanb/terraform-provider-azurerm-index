func expandCdnFrontDoorFirewallTransforms(input []interface{}) *[]waf.TransformType {
	result := make([]waf.TransformType, 0)
	if len(input) == 0 {
		return nil
	}

	for _, v := range input {
		result = append(result, waf.TransformType(v.(string)))
	}

	return &result
}
