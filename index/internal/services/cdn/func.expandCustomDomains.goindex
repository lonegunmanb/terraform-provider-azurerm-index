func expandCustomDomains(input []interface{}) ([]interface{}, error) {
	out := make([]interface{}, 0)
	if len(input) == 0 || input == nil {
		return out, nil
	}

	for _, v := range input {
		id, err := parse.FrontDoorCustomDomainID(v.(string))
		if err != nil {
			return nil, err
		}

		out = append(out, id.ID())
	}

	return out, nil
}
