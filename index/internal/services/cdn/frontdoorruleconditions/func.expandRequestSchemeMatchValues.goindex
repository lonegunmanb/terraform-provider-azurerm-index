func expandRequestSchemeMatchValues(input []interface{}) *[]rules.RequestSchemeMatchValue {
	result := make([]rules.RequestSchemeMatchValue, 0)
	if len(input) == 0 {
		return nil
	}

	for _, v := range input {
		matchValue := v.(string)
		result = append(result, rules.RequestSchemeMatchValue(matchValue))
	}

	return &result
}
