func flattenRequestSchemeMatchValues(input *[]rules.RequestSchemeMatchValue) *[]string {
	result := make([]string, 0)
	if input == nil {
		return nil
	}

	for _, v := range *input {
		matchValue := string(v)
		result = append(result, matchValue)
	}

	return &result
}
