func getIds(subResource *[]ipgroups.SubResource) []string {
	if subResource == nil {
		return nil
	}

	ids := make([]string, 0)
	for _, v := range *subResource {
		if v.Id == nil {
			continue
		}

		ids = append(ids, *v.Id)
	}

	return ids
}
