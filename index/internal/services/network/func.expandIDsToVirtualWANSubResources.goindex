func expandIDsToVirtualWANSubResources(input []interface{}) *[]virtualwans.SubResource {
	ids := make([]virtualwans.SubResource, 0)

	for _, v := range input {
		ids = append(ids, virtualwans.SubResource{
			Id: pointer.To(v.(string)),
		})
	}

	return &ids
}
