func flattenExpressRouteCircuitSku(sku *expressroutecircuits.ExpressRouteCircuitSku) []interface{} {
	if sku == nil {
		return []interface{}{}
	}

	return []interface{}{
		map[string]interface{}{
			"tier":   string(pointer.From(sku.Tier)),
			"family": string(pointer.From(sku.Family)),
		},
	}
}
