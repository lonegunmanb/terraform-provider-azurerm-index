func flattenVirtualWanProperties(input *[]virtualwans.SubResource) []interface{} {
	if input == nil {
		return []interface{}{}
	}
	output := make([]interface{}, 0)
	for _, v := range *input {
		if v.Id != nil {
			output = append(output, *v.Id)
		}
	}
	return output
}
