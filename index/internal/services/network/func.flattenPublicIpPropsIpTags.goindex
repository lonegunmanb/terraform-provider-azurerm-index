func flattenPublicIpPropsIpTags(input *[]publicipaddresses.IPTag) map[string]interface{} {
	out := make(map[string]interface{})

	if input != nil {
		for _, tag := range *input {
			if tag.IPTagType != nil {
				out[*tag.IPTagType] = tag.Tag
			}
		}
	}

	return out
}
