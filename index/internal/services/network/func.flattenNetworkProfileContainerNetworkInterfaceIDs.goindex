func flattenNetworkProfileContainerNetworkInterfaceIDs(input *[]networkprofiles.ContainerNetworkInterface) []string {
	retCNIs := make([]string, 0)

	if input != nil {
		for _, retCNI := range *input {
			if retCNI.Id != nil {
				retCNIs = append(retCNIs, *retCNI.Id)
			}
		}
	}

	return retCNIs
}
