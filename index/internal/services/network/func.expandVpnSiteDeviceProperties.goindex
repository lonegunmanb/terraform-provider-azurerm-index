func expandVpnSiteDeviceProperties(vendor, model string) *virtualwans.DeviceProperties {
	if vendor == "" && model == "" {
		return nil
	}
	output := &virtualwans.DeviceProperties{}
	if vendor != "" {
		output.DeviceVendor = &vendor
	}
	if model != "" {
		output.DeviceModel = &model
	}

	return output
}
