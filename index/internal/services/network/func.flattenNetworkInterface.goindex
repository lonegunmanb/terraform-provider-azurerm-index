func flattenNetworkInterface(networkInterfaceId string) interface{} {
	id, err := commonids.ParseNetworkInterfaceID(networkInterfaceId)
	if err != nil {
		return []interface{}{}
	}

	return []interface{}{
		map[string]interface{}{
			"id":   id.ID(),
			"name": id.NetworkInterfaceName,
		},
	}
}
