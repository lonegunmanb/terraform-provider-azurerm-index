func flattenNetworkManagerScopeAccesses(input *[]networkmanagers.ConfigurationType) []string {
	result := make([]string, 0)
	if input == nil {
		return result
	}

	for _, v := range *input {
		result = append(result, string(v))
	}
	return result
}
