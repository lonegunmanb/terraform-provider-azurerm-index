func (br assignmentBaseResource) expandStringSlice(in interface{}) (res []string) {
	if in == nil {
		return nil
	}
	if slice, ok := in.([]interface{}); ok {
		for _, v := range slice {
			if v != nil {
				res = append(res, v.(string))
			} else {
				res = append(res, "")
			}
		}
	}
	return res
}
