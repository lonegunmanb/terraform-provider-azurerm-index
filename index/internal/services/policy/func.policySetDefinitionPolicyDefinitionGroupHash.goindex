func policySetDefinitionPolicyDefinitionGroupHash(v interface{}) int {
	var buf bytes.Buffer

	if m, ok := v.(map[string]interface{}); ok {
		buf.WriteString(m["name"].(string))
	}

	return pluginsdk.HashString(buf.String())
}
