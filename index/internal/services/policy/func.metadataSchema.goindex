func metadataSchema() *pluginsdk.Schema {
	return &pluginsdk.Schema{
		Type:             pluginsdk.TypeString,
		Optional:         true,
		Computed:         true,
		ValidateFunc:     validation.StringIsJSON,
		DiffSuppressFunc: metadataDiffSuppressFunc,
	}
}
