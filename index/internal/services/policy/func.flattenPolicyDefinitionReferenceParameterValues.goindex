func flattenPolicyDefinitionReferenceParameterValues(input *map[string]policysetdefinitions.ParameterValuesValue) (string, error) {
	if input == nil {
		return "", nil
	}

	result, err := json.Marshal(input)
	if err != nil {
		return "", err
	}

	return string(result), err
}
