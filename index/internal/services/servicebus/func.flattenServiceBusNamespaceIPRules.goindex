func flattenServiceBusNamespaceIPRules(input *[]namespaces.NWRuleSetIPRules) []interface{} {
	if input == nil || len(*input) == 0 {
		return []interface{}{}
	}

	result := make([]interface{}, 0, len(*input))
	for _, v := range *input {
		if v.IPMask != nil {
			result = append(result, *v.IPMask)
		}
	}

	return result
}
