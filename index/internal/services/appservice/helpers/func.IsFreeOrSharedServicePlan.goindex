func IsFreeOrSharedServicePlan(inputSKU string) bool {
	result := false
	for _, sku := range freeSkus {
		if inputSKU == sku {
			result = true
		}
	}
	for _, sku := range sharedSkus {
		if inputSKU == sku {
			result = true
		}
	}
	return result
}
