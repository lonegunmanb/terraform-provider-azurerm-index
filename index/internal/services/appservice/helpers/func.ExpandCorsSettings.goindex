func ExpandCorsSettings(input []CorsSetting) *webapps.CorsSettings {
	if len(input) != 1 {
		return &webapps.CorsSettings{}
	}
	cors := input[0]

	return &webapps.CorsSettings{
		AllowedOrigins:     pointer.To(cors.AllowedOrigins),
		SupportCredentials: pointer.To(cors.SupportCredentials),
	}
}
