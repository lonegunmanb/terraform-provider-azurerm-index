func flattenDataDisks(input *map[string][]string) []DataDisk {
	results := make([]DataDisk, 0)
	if input == nil {
		return results
	}

	for k, v := range *input {
		dataDisk := DataDisk{
			VolumeName: k,
			Names:      v,
		}

		results = append(results, dataDisk)
	}

	return results
}
