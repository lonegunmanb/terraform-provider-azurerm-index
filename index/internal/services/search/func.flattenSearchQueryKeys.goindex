func flattenSearchQueryKeys(input *[]querykeys.QueryKey) []interface{} {
	results := make([]interface{}, 0)

	if input != nil {
		for _, v := range *input {
			results = append(results, map[string]interface{}{
				"name": pointer.From(v.Name),
				"key":  pointer.From(v.Key),
			})
		}
	}

	return results
}
