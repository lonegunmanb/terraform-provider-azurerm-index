func flattenHDInsightSparkComponentVersion(input *map[string]string) []interface{} {
	output := make([]interface{}, 0)
	if input != nil {
		sparkVersion := ""
		if v, ok := (*input)["Spark"]; ok {
			sparkVersion = v
		}
		output = append(output, map[string]interface{}{
			"spark": sparkVersion,
		})
	}
	return output
}
