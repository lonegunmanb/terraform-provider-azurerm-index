func expandHDInsightKafkaComponentVersion(input []interface{}) map[string]string {
	kafkaVersion := ""
	if len(input) > 0 && input[0] != nil {
		vs := input[0].(map[string]interface{})
		kafkaVersion = vs["kafka"].(string)
	}
	return map[string]string{
		"kafka": kafkaVersion,
	}
}
