func expandModules(input []ModuleModel) *[]databases.Module {
	results := make([]databases.Module, 0, len(input))
	for _, module := range input {
		results = append(results, databases.Module{
			Name: module.Name,
			Args: pointer.To(module.Args),
		})
	}
	return &results
}
