func expandDeviceSku(input string) *devices.Sku {
	if len(input) == 0 {
		return nil
	}

	v, err := parse.DataboxEdgeDeviceSkuName(input)
	if err != nil {
		return nil
	}

	return &devices.Sku{
		Name: pointer.To(devices.SkuName(v.Name)),
		Tier: pointer.To(devices.SkuTier(v.Tier)),
	}
}
