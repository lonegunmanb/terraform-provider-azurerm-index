func expandAzureRmDnsNsRecords(input []interface{}) *[]recordsets.NsRecord {
	records := make([]recordsets.NsRecord, 0)
	for _, v := range input {
		record := v.(string)

		records = append(records, recordsets.NsRecord{
			Nsdname: &record,
		})
	}
	return &records
}
