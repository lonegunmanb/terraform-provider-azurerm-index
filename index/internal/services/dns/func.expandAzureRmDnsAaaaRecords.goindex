func expandAzureRmDnsAaaaRecords(input []interface{}) *[]recordsets.AaaaRecord {
	records := make([]recordsets.AaaaRecord, len(input))

	for i, v := range input {
		ipv6 := NormalizeIPv6Address(v)
		records[i] = recordsets.AaaaRecord{
			IPv6Address: &ipv6,
		}
	}

	return &records
}
