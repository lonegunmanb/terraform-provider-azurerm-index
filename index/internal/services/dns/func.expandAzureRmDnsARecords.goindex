func expandAzureRmDnsARecords(input []interface{}) *[]recordsets.ARecord {
	records := make([]recordsets.ARecord, len(input))

	for i, v := range input {
		ipv4 := v.(string)
		records[i] = recordsets.ARecord{
			IPv4Address: &ipv4,
		}
	}

	return &records
}
