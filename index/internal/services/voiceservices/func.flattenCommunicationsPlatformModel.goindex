func flattenCommunicationsPlatformModel(input []communicationsgateways.CommunicationsPlatform) []string {
	output := make([]string, 0)
	if len(input) == 0 {
		return nil
	}

	for _, v := range input {
		output = append(output, string(v))
	}

	return output
}
