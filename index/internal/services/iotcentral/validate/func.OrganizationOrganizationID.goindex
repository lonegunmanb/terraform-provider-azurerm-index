package github.com/hashicorp/terraform-provider-azurerm/internal/services/iotcentral/validate
import (
	"fmt"
	"regexp"
)
func OrganizationOrganizationID(i interface{}, k string) (warnings []string, errors []error) {
	id, ok := i.(string)
	if !ok {
		errors = append(errors, fmt.Errorf("expected %s to be a string", k))
		return warnings, errors
	}

	err := validateOrganizationId(id)
	if err != nil {
		errors = append(errors, err)
		return warnings, errors
	}

	return warnings, errors
}
