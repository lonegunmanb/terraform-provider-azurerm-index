func expandMetricRules(input []interface{}) *rules.MetricRules {
	if len(input) == 0 {
		return nil
	}
	v := input[0].(map[string]interface{})
	filteringTag := v["filter"].([]interface{})

	return &rules.MetricRules{
		FilteringTags: expandFilteringTag(filteringTag),
	}
}
