func flattenMetricRules(input *rules.MetricRules) []interface{} {
	if input == nil {
		return make([]interface{}, 0)
	}

	return []interface{}{
		map[string]interface{}{
			"filter": flattenFilteringTags(input.FilteringTags),
		},
	}
}
