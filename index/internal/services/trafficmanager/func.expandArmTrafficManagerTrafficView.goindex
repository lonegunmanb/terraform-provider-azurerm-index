func expandArmTrafficManagerTrafficView(s bool) *profiles.TrafficViewEnrollmentStatus {
	enabled := profiles.TrafficViewEnrollmentStatusDisabled
	if s {
		enabled = profiles.TrafficViewEnrollmentStatusEnabled
	}
	return &enabled
}
