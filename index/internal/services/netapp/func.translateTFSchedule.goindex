func translateTFSchedule(scheduleName string) string {
	if strings.EqualFold(scheduleName, string(netAppModels.ReplicationSchedule10Minutes)) {
		return string(volumegroups.ReplicationScheduleOneZerominutely)
	}

	return scheduleName
}
