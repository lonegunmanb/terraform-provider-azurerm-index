func expandTemplateDeploymentDebugSetting(debugLevel string) *resources.DebugSetting {
	if debugLevel == "" {
		return &resources.DebugSetting{
			DetailLevel: pointer.To(string(debugLevelNone)),
		}
	}

	return &resources.DebugSetting{
		DetailLevel: pointer.To(debugLevel),
	}
}
