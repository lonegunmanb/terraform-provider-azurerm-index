func hashEnvironmentVariables(v interface{}) int {
	var buf bytes.Buffer
	m := v.(map[string]interface{})
	buf.WriteString(fmt.Sprintf("%s-", strings.ToLower(m["name"].(string))))
	return pluginsdk.HashString(buf.String())
}
