func flattenLighthouseDefinitionPlan(input *registrationdefinitions.Plan) []interface{} {
	if input == nil {
		return []interface{}{}
	}

	return []interface{}{
		map[string]interface{}{
			"name":      input.Name,
			"publisher": input.Publisher,
			"product":   input.Product,
			"version":   input.Version,
		},
	}
}
