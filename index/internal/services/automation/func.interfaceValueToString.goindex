package github.com/hashicorp/terraform-provider-azurerm/internal/services/automation
import (
	"fmt"
)
func interfaceValueToString(v interface{}) (string, error) {
	switch value := v.(type) {
	case string:
		return value, nil
	case int:
		return fmt.Sprintf("%d", value), nil
	default:
		return "", fmt.Errorf("unknown type %T in value", value)
	}
}
