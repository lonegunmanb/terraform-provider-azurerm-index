func expandPowerShell72ModuleLink(m []ModuleLinkModel) module.ContentLink {
	moduleLink := m[0]
	if len(moduleLink.Hash) > 0 {
		hash := moduleLink.Hash[0]
		return module.ContentLink{
			Uri: &moduleLink.Uri,
			ContentHash: &module.ContentHash{
				Algorithm: hash.Algorithm,
				Value:     hash.Value,
			},
		}
	}

	return module.ContentLink{
		Uri: &moduleLink.Uri,
	}
}
