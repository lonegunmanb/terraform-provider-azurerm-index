func (r ElasticSANVolumeSnapshotDataSource) Attributes() map[string]*pluginsdk.Schema {
	return map[string]*pluginsdk.Schema{
		"source_id": {
			Type:     pluginsdk.TypeString,
			Computed: true,
		},

		"source_volume_size_in_gib": {
			Computed: true,
			Type:     pluginsdk.TypeInt,
		},

		"volume_name": {
			Computed: true,
			Type:     pluginsdk.TypeString,
		},
	}
}
