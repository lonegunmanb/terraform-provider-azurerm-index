func ExpandSku(input []ElasticSANResourceSkuModel) elasticsans.Sku {
	if len(input) == 0 {
		return elasticsans.Sku{}
	}

	output := elasticsans.Sku{
		Name: elasticsans.SkuName(input[0].Name),
	}

	if input[0].Tier != "" {
		output.Tier = pointer.To(elasticsans.SkuTier(input[0].Tier))
	}

	return output
}
