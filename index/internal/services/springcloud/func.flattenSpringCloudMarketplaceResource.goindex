func flattenSpringCloudMarketplaceResource(input *appplatform.MarketplaceResource) []interface{} {
	if input == nil {
		return []interface{}{}
	}
	plan := ""
	publisher := ""
	product := ""
	if input.Plan != nil {
		plan = *input.Plan
	}
	if input.Publisher != nil {
		publisher = *input.Publisher
	}
	if input.Product != nil {
		product = *input.Product
	}
	return []interface{}{
		map[string]interface{}{
			"plan":      plan,
			"publisher": publisher,
			"product":   product,
		},
	}
}
