func flattenNewRelicLabels(input string) map[string]string {
	if input == "" {
		return nil
	}
	labels := make(map[string]string)
	for _, label := range strings.Split(input, ";") {
		parts := strings.Split(label, ":")
		if len(parts) == 2 {
			labels[parts[0]] = parts[1]
		}
	}
	return labels
}
