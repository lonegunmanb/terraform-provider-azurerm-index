func flattenGatewayRouteConfigOpenApi(input *appplatform.GatewayRouteConfigOpenAPIProperties) interface{} {
	if input == nil {
		return []interface{}{}
	}

	uri := ""
	if input.URI != nil {
		uri = *input.URI
	}

	return []interface{}{
		map[string]interface{}{
			"uri": uri,
		},
	}
}
