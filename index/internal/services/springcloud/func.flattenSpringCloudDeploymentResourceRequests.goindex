func flattenSpringCloudDeploymentResourceRequests(input *appplatform.ResourceRequests) []interface{} {
	if input == nil {
		return []interface{}{}
	}

	cpu := ""
	if input.CPU != nil {
		cpu = *input.CPU
	}

	memory := ""
	if input.Memory != nil {
		memory = *input.Memory
	}

	return []interface{}{
		map[string]interface{}{
			"cpu":    cpu,
			"memory": memory,
		},
	}
}
