func flattenGatewayApms(input *[]appplatform.ApmReference) ([]string, error) {
	out := make([]string, 0)
	if input == nil {
		return out, nil
	}
	for _, v := range *input {
		id, err := appplatform.ParseApmIDInsensitively(v.ResourceId)
		if err != nil {
			return nil, err
		}
		out = append(out, id.ID())
	}
	return out, nil
}
