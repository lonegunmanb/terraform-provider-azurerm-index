func flattenSpringCloudAPIPortalGatewayIds(ids *[]string) []string {
	if ids == nil || len(*ids) == 0 {
		return nil
	}
	out := make([]string, 0)
	for _, id := range *ids {
		gatewayId, err := appplatform.ParseGatewayIDInsensitively(id)
		if err == nil {
			out = append(out, gatewayId.ID())
		}
	}
	return out
}
