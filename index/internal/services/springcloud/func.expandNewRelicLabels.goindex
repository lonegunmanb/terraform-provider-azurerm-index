func expandNewRelicLabels(input map[string]string) string {
	if len(input) == 0 {
		return ""
	}
	labels := make([]string, 0)
	for k, v := range input {
		labels = append(labels, fmt.Sprintf("%s:%s", k, v))
	}
	return strings.Join(labels, ";")
}
