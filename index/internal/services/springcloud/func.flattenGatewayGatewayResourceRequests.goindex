func flattenGatewayGatewayResourceRequests(input *appplatform.GatewayResourceRequests) []QuotaModel {
	if input == nil {
		return make([]QuotaModel, 0)
	}
	return []QuotaModel{
		{
			Cpu:    pointer.From(input.Cpu),
			Memory: pointer.From(input.Memory),
		},
	}
}
