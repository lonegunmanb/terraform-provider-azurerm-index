package github.com/hashicorp/terraform-provider-azurerm/internal/services/extendedlocation
import (
	"github.com/hashicorp/terraform-plugin-framework/action"
	"github.com/hashicorp/terraform-plugin-framework/ephemeral"
	"github.com/hashicorp/terraform-provider-azurerm/internal/features"
	"github.com/hashicorp/terraform-provider-azurerm/internal/sdk"
)
func (r Registration) Resources() []sdk.Resource {
	if !features.FivePointOh() {
		return []sdk.Resource{
			CustomLocationResource{}, // this resource is renamed and should be removed in 5.0
			ExtendedLocationCustomLocationResource{},
		}
	}

	return []sdk.Resource{
		ExtendedLocationCustomLocationResource{},
	}
}
