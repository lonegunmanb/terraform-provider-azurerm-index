func compactParameterOrOutputValue(v interface{}) (string, error) {
	result, err := json.Marshal(v)
	if err != nil {
		return "", err
	}

	compactJson := bytes.Buffer{}
	if err = json.Compact(&compactJson, result); err != nil {
		return "", err
	}
	return compactJson.String(), nil
}
