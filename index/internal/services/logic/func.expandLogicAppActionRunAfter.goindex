func expandLogicAppActionRunAfter(input []interface{}) map[string]interface{} {
	if len(input) == 0 {
		return nil
	}
	output := map[string]interface{}{}
	for _, v := range input {
		b := v.(map[string]interface{})
		output[b["action_name"].(string)] = []string{b["action_result"].(string)}
	}

	return output
}
