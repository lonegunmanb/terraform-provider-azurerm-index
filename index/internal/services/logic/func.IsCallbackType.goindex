func IsCallbackType(tType string) bool {
	cTypes := []string{"ApiConnectionWebhook", "HTTPWebhook", "Request"}

	valid := validation.StringInSlice(cTypes, false)
	_, errors := valid(tType, "callback_url")

	return len(errors) == 0
}
