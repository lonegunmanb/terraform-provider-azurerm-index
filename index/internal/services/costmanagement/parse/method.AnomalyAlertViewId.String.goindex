package github.com/hashicorp/terraform-provider-azurerm/internal/services/costmanagement/parse
import (
	"errors"
	"fmt"
	"strings"

	"github.com/hashicorp/go-azure-helpers/resourcemanager/resourceids"
)
func (id AnomalyAlertViewId) String() string {
	segments := []string{
		fmt.Sprintf("View Name %q", id.ViewName),
	}
	segmentsStr := strings.Join(segments, " / ")
	return fmt.Sprintf("%s: (%s)", "Anomaly Alert View", segmentsStr)
}
