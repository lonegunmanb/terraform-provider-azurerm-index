func ExpandBatchMetaData(input map[string]interface{}) *[]pool.MetadataItem {
	output := []pool.MetadataItem{}

	for k, v := range input {
		name := k
		value := v.(string)
		output = append(output, pool.MetadataItem{
			Name:  name,
			Value: value,
		})
	}

	return &output
}
