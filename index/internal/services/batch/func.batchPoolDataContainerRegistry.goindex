func batchPoolDataContainerRegistry() map[string]*schema.Schema {
	return map[string]*pluginsdk.Schema{
		"registry_server": {
			Type:     pluginsdk.TypeString,
			Computed: true,
		},
		"user_assigned_identity_id": {
			Type:     pluginsdk.TypeString,
			Computed: true,
		},
		"user_name": {
			Type:     pluginsdk.TypeString,
			Computed: true,
		},
		"password": {
			Type:      pluginsdk.TypeString,
			Computed:  true,
			Sensitive: true,
		},
	}
}
