func isShardKeyAllowed(input []interface{}) bool {
	if len(input) == 0 {
		return false
	}
	for _, authMod := range input {
		if strings.EqualFold(authMod.(string), string(batchaccount.AuthenticationModeSharedKey)) {
			return true
		}
	}
	return false
}
