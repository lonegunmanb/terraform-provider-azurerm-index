func flattenSqlVirtualMachineAutoBackupDaysOfWeek(daysOfWeek *[]sqlvirtualmachines.AutoBackupDaysOfWeek) []interface{} {
	output := make([]interface{}, 0)

	if daysOfWeek != nil {
		for _, v := range *daysOfWeek {
			output = append(output, string(v))
		}
	}

	return output
}
