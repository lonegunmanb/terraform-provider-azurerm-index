func flattenSqlServerRestorableDatabases(resp restorabledroppeddatabases.ListByServerCompleteResult) []string {
	if len(resp.Items) == 0 {
		return []string{}
	}

	res := make([]string, 0)
	for _, r := range resp.Items {
		var id string
		if r.Id != nil {
			id = *r.Id
			res = append(res, id)
		}
	}

	return res
}
