func (br consumptionBudgetBaseResource) importerFunc() sdk.ResourceRunFunc {
	return func(ctx context.Context, metadata sdk.ResourceMetaData) error {
		_, err := budgets.ParseScopedBudgetID(metadata.ResourceData.Id())
		if err != nil {
			return err
		}

		return nil
	}
}
