func expandPortRange(inputList []PortRangeModel) *attacheddatanetwork.PortRange {
	if len(inputList) == 0 {
		return nil
	}

	input := &inputList[0]
	output := attacheddatanetwork.PortRange{
		MaxPort: &input.Maximum,
		MinPort: &input.Minimum,
	}

	return &output
}
