func flattenRedisEnterpriseClusterSku(input redisenterprise.Sku) *string {
	var name redisenterprise.SkuName
	var capacity int64

	if input.Name != "" {
		name = input.Name
	}

	if input.Capacity != nil {
		capacity = *input.Capacity
	}

	skuName := fmt.Sprintf("%s-%d", name, capacity)

	return &skuName
}
