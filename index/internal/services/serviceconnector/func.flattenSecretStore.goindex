func flattenSecretStore(input servicelinker.SecretStore) []SecretStoreModel {
	var keyVaultId string
	if input.KeyVaultId != nil {
		keyVaultId = *input.KeyVaultId
	}

	return []SecretStoreModel{
		{
			KeyVaultId: keyVaultId,
		},
	}
}
