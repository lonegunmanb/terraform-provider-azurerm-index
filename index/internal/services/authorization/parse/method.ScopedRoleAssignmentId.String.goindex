package github.com/hashicorp/terraform-provider-azurerm/internal/services/authorization/parse
import (
	"fmt"
	"strings"

	"github.com/hashicorp/go-azure-sdk/resource-manager/authorization/2022-04-01/roleassignments"
)
func (id ScopedRoleAssignmentId) String() string {
	components := []string{
		id.ScopedId.String(),
	}

	if id.TenantId != "" {
		components = append(components, "Tenant ID: "+id.TenantId)
	}

	return fmt.Sprintf("Scoped Role Assignment (%s)", strings.Join(components, "\n"))
}
