func flattenDomainInboundIPRules(input *[]domains.InboundIPRule) []interface{} {
	rules := make([]interface{}, 0)
	if input == nil {
		return rules
	}

	for _, r := range *input {
		action := ""
		if r.Action != nil {
			action = string(*r.Action)
		}

		rules = append(rules, map[string]interface{}{
			"action":  action,
			"ip_mask": pointer.From(r.IPMask),
		})
	}
	return rules
}
