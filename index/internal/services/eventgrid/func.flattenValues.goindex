func flattenValues(inputKey *string, inputValues *[]any) map[string]any {
	key := ""
	if inputKey != nil {
		key = *inputKey
	}
	values := make([]any, 0)
	if inputValues != nil {
		values = *inputValues
	}

	return map[string]any{
		"key":    key,
		"values": values,
	}
}
