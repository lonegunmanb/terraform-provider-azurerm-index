func flattenBackupProtectionPolicyVMWorkloadRetentionDailyFormat(input *protectionpolicies.DailyRetentionFormat) []int64 {
	result := make([]int64, 0)

	if days := input.DaysOfTheMonth; days != nil {
		for _, d := range *days {
			result = append(result, pointer.From(d.Date))
		}
	}

	return result
}
