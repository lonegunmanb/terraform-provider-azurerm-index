func expandAlertRuleMsSecurityIncidentSeverityFilter(input []interface{}) *[]alertrules.AlertSeverity {
	result := make([]alertrules.AlertSeverity, 0)

	for _, e := range input {
		result = append(result, alertrules.AlertSeverity(e.(string)))
	}

	return &result
}
