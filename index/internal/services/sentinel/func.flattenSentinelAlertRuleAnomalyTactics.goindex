func flattenSentinelAlertRuleAnomalyTactics(input *[]securityinsight.AttackTactic) []string {
	if input == nil {
		return []string{}
	}

	output := make([]string, 0)
	for _, v := range *input {
		output = append(output, string(v))
	}

	return output
}
