func expandAlertRuleTactics(input []interface{}) *[]alertrules.AttackTactic {
	result := make([]alertrules.AttackTactic, 0)

	for _, e := range input {
		result = append(result, alertrules.AttackTactic(e.(string)))
	}

	return &result
}
