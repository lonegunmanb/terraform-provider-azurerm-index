func AnomalyRuleSingleSelectSchema() *pluginsdk.Schema {
	return &pluginsdk.Schema{
		Type:     pluginsdk.TypeList,
		Computed: true,
		Elem: &schema.Resource{
			Schema: map[string]*schema.Schema{
				"name": {
					Type:     pluginsdk.TypeString,
					Computed: true,
				},
				"description": {
					Type:     pluginsdk.TypeString,
					Computed: true,
				},
				"supported_values": {
					Type:     pluginsdk.TypeList,
					Computed: true,
					Elem: &schema.Schema{
						Type: pluginsdk.TypeString,
					},
				},
				"value": {
					Type:     pluginsdk.TypeString,
					Computed: true,
				},
			},
		},
	}
}
