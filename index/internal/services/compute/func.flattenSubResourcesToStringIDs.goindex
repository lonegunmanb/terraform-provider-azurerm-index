func flattenSubResourcesToStringIDs(input *[]virtualmachinescalesets.SubResource) []string {
	ids := make([]string, 0)
	if input == nil {
		return ids
	}

	for _, v := range *input {
		if v.Id == nil {
			continue
		}

		ids = append(ids, *v.Id)
	}

	return ids
}
