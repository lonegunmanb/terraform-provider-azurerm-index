func expandIDsToSubResources(input []interface{}) *[]virtualmachinescalesets.SubResource {
	ids := make([]virtualmachinescalesets.SubResource, 0)

	for _, v := range input {
		ids = append(ids, virtualmachinescalesets.SubResource{
			Id: pointer.To(v.(string)),
		})
	}

	return &ids
}
