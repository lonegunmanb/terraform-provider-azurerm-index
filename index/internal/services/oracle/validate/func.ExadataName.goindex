package github.com/hashicorp/terraform-provider-azurerm/internal/services/oracle/validate
import (
	"fmt"
	"slices"

	"github.com/hashicorp/go-azure-sdk/resource-manager/oracledatabase/2025-03-01/cloudexadatainfrastructures"
)
func ExadataName(i interface{}, k string) (warnings []string, errors []error) {
	v, ok := i.(string)
	if !ok {
		errors = append(errors, fmt.Errorf("expected type of %s to be string", k))
		return
	}

	if v == "" {
		errors = append(errors, fmt.Errorf("%v must not be an empty string", k))
		return
	}

	return
}
