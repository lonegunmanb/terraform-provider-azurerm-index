package github.com/hashicorp/terraform-provider-azurerm/internal/services/oracle/validate
import (
	"fmt"
	"regexp"
	"unicode"
	"unicode/utf8"

	"github.com/hashicorp/go-azure-sdk/resource-manager/oracledatabase/2025-03-01/cloudvmclusters"
)
func CpuCoreCount(i interface{}, k string) (warnings []string, errors []error) {
	v, ok := i.(int)
	if !ok {
		errors = append(errors, fmt.Errorf("expected type of %s to be int", k))
		return
	}

	if v < 2 {
		errors = append(errors, fmt.Errorf("cpu_core_count must be at least %v", 2))
		return
	}

	return
}
