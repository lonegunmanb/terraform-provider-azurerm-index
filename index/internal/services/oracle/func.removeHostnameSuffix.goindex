func removeHostnameSuffix(hostnameActual string) string {
	suffixIndex := strings.LastIndex(hostnameActual, "-")
	if suffixIndex != -1 {
		return hostnameActual[:suffixIndex]
	} else {
		return hostnameActual
	}
}
