func NewHostnameBindingID(subscriptionId, resourceGroup, siteName, name string) HostnameBindingId {
	return HostnameBindingId{
		SubscriptionId: subscriptionId,
		ResourceGroup:  resourceGroup,
		SiteName:       siteName,
		Name:           name,
	}
}
