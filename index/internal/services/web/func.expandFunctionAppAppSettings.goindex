func expandFunctionAppAppSettings(d *pluginsdk.ResourceData, basicAppSettings []web.NameValuePair) map[string]*string {
	output := expandAppServiceAppSettings(d)

	for _, p := range basicAppSettings {
		output[*p.Name] = p.Value
	}

	return output
}
