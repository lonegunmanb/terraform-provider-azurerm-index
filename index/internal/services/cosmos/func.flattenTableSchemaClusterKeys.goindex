func flattenTableSchemaClusterKeys(input *[]documentdb.ClusterKey) []interface{} {
	if input == nil {
		return nil
	}

	keys := make([]interface{}, 0)

	for _, v := range *input {
		name := ""
		if v.Name != nil {
			name = *v.Name
		}
		orderBy := ""
		if v.OrderBy != nil {
			orderBy = *v.OrderBy
		}
		keys = append(keys, map[string]interface{}{
			"name":     name,
			"order_by": orderBy,
		})
	}

	return keys
}
