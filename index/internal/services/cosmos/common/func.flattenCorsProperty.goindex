func flattenCorsProperty(input *string) []interface{} {
	if input == nil {
		return make([]interface{}, 0)
	}

	results := make([]interface{}, 0, len(*input))

	origins := strings.Split(*input, ",")
	for _, origin := range origins {
		results = append(results, origin)
	}

	return results
}
