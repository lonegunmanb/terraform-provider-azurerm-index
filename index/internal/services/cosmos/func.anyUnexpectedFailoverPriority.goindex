func anyUnexpectedFailoverPriority(failoverPolicies []cosmosdb.FailoverPolicy) bool {
	size := len(failoverPolicies)
	for _, policy := range failoverPolicies {
		if int(*policy.FailoverPriority) > size-1 {
			return true
		}
	}
	return false
}
