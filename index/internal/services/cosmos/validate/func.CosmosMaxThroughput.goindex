package github.com/hashicorp/terraform-provider-azurerm/internal/services/cosmos/validate
import (
	"fmt"
	"regexp"
)
func CosmosMaxThroughput(i interface{}, k string) (warnings []string, errors []error) {
	v, ok := i.(int)
	if !ok {
		errors = append(errors, fmt.Errorf("expected type of %q to be int", k))
		return
	}

	if v < 1000 {
		errors = append(errors, fmt.Errorf(
			"%s must be a minimum of 1000", k))
	}

	if v%1000 != 0 {
		errors = append(errors, fmt.Errorf(
			"%q must be set in increments of 1000", k))
	}

	return warnings, errors
}
