func flattenCassandraClusterCertificate(input *[]managedcassandras.Certificate) []interface{} {
	results := make([]interface{}, 0)
	if input == nil {
		return results
	}

	for _, item := range *input {
		var pem string
		if item.Pem != nil {
			pem = *item.Pem
		}

		results = append(results, pem)
	}

	return results
}
