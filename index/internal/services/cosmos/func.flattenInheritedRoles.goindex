func flattenInheritedRoles(input *[]mongorbacs.Role) []string {
	if input == nil {
		return []string{}
	}

	result := make([]string, 0, len(*input))
	for _, v := range *input {
		result = append(result, pointer.From(v.Role))
	}

	return result
}
