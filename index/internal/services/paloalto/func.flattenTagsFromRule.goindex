func flattenTagsFromRule(input *[]localrules.TagInfo) map[string]interface{} {
	if input == nil {
		return map[string]interface{}{}
	}

	result := make(map[string]interface{})
	for _, v := range *input {
		result[v.Key] = v.Value
	}

	return result
}
