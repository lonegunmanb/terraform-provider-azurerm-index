package github.com/hashicorp/terraform-provider-azurerm/internal/services/containerapps/helpers
import (
	"reflect"
	"strings"

	"github.com/hashicorp/go-azure-helpers/lang/pointer"
	"github.com/hashicorp/go-azure-sdk/resource-manager/containerapps/2024-02-02-preview/jobs"
	"github.com/hashicorp/terraform-provider-azurerm/internal/tf/pluginsdk"
	"github.com/hashicorp/terraform-provider-azurerm/internal/tf/validation"
)
func UnpackContainerJobSecretsCollection(input *jobs.JobSecretsCollection) *[]jobs.Secret {
	if input == nil || len(input.Value) == 0 {
		return nil
	}

	result := make([]jobs.Secret, 0)
	result = append(result, input.Value...)

	return &result
}
