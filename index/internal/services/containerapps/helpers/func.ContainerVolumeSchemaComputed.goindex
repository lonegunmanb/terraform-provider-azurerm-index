func ContainerVolumeSchemaComputed() *pluginsdk.Schema {
	return &pluginsdk.Schema{
		Type:     pluginsdk.TypeList,
		Computed: true,
		Elem: &pluginsdk.Resource{
			Schema: map[string]*pluginsdk.Schema{
				"name": {
					Type:     pluginsdk.TypeString,
					Computed: true,
				},

				"storage_type": {
					Type:     pluginsdk.TypeString,
					Computed: true,
				},

				"storage_name": {
					Type:     pluginsdk.TypeString,
					Computed: true,
				},

				"mount_options": {
					Type:     pluginsdk.TypeString,
					Computed: true,
				},
			},
		},
	}
}
