func NewBackendPoolID(subscriptionId, resourceGroup, frontDoorName, name string) BackendPoolId {
	return BackendPoolId{
		SubscriptionId: subscriptionId,
		ResourceGroup:  resourceGroup,
		FrontDoorName:  frontDoorName,
		Name:           name,
	}
}
