func expandSavedSearchTag(input map[string]interface{}) *[]savedsearches.Tag {
	results := make([]savedsearches.Tag, 0)
	for key, value := range input {
		result := savedsearches.Tag{
			Name:  key,
			Value: value.(string),
		}
		results = append(results, result)
	}
	return &results
}
