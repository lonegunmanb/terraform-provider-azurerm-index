func flattenLogAnalyticsQueryPackQueryTags(input map[string][]string) map[string]string {
	if input == nil {
		return nil
	}

	results := make(map[string]string)
	for k, v := range input {
		results[k] = strings.Join(v, ",")
	}

	return results
}
