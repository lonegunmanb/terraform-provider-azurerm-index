package github.com/hashicorp/terraform-provider-azurerm/internal/services/loganalytics
import "github.com/hashicorp/terraform-provider-azurerm/utils"
func expandTags(input map[string]interface{}) *map[string]string {
	output := make(map[string]string)
	for k, v := range input {
		output[k] = v.(string)
	}
	return &output
}
