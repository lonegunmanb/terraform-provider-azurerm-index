package github.com/hashicorp/terraform-provider-azurerm/internal/services/storage/validate
import (
	"fmt"
)
func StorageBlobIndexTagValue(v interface{}, k string) (warnings []string, errors []error) {
	value := v.(string)
	if len(value) > 256 {
		errors = append(errors, fmt.Errorf(
			"%q must be between 0 and 256 characters: %q", k, value))
	}
	return warnings, errors
}
