func (w DataPlaneStorageShareWrapper) Delete(ctx context.Context, shareName string) error {
	input := shares.DeleteInput{
		DeleteSnapshots: true,
	}
	_, err := w.client.Delete(ctx, shareName, input)
	return err
}
