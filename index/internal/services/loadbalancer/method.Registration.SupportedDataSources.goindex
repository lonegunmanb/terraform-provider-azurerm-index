		"azurerm_lb_backend_address_pool": dataSourceArmLoadBalancerBackendAddressPool(),
		"azurerm_lb_rule":                 dataSourceArmLoadBalancerRule(),
		"azurerm_lb_outbound_rule":        dataSourceArmLoadBalancerOutboundRule(),
	}
}
