func expandPreviewFeatures(input []string) *[]mongoclusters.PreviewFeature {
	if len(input) == 0 {
		return nil
	}

	result := make([]mongoclusters.PreviewFeature, 0)

	for _, v := range input {
		if v != "" {
			result = append(result, mongoclusters.PreviewFeature(v))
		}
	}

	return &result
}
