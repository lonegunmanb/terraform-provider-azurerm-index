func flattenDatabaseRoles(input *[]users.DatabaseRole) []DatabaseRoleModel {
	results := make([]DatabaseRoleModel, 0)
	if input == nil {
		return results
	}

	for _, v := range *input {
		results = append(results, DatabaseRoleModel{
			Database: v.Db,
			Name:     string(v.Role),
		})
	}

	return results
}
