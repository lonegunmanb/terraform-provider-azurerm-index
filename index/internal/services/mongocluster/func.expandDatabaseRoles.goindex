func expandDatabaseRoles(input []DatabaseRoleModel) *[]users.DatabaseRole {
	if len(input) == 0 {
		return nil
	}

	result := make([]users.DatabaseRole, 0)
	for _, v := range input {
		result = append(result, users.DatabaseRole{
			Db:   v.Database,
			Role: users.UserRole(v.Name),
		})
	}

	return &result
}
