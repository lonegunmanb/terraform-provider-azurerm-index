func expandGatewaySkuModel(inputList []GatewaySkuModel) *apigateway.ApiManagementGatewaySkuProperties {
	if len(inputList) == 0 {
		return nil
	}

	input := &inputList[0]
	return &apigateway.ApiManagementGatewaySkuProperties{
		Capacity: pointer.To(input.Capacity),
		Name:     input.Name,
	}
}
