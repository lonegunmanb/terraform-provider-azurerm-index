func apiManagementDataSourceHostnameSchema() map[string]*pluginsdk.Schema {
	return map[string]*pluginsdk.Schema{
		"host_name": {
			Type:     pluginsdk.TypeString,
			Computed: true,
		},

		"key_vault_id": {
			Type:     pluginsdk.TypeString,
			Computed: true,
		},

		"negotiate_client_certificate": {
			Type:     pluginsdk.TypeBool,
			Computed: true,
		},
	}
}
