func expandApiManagementApiLicense(input []interface{}) *api.ApiLicenseInformation {
	if len(input) == 0 {
		return nil
	}

	v := input[0].(map[string]interface{})
	return &api.ApiLicenseInformation{
		Name: pointer.To(v["name"].(string)),
		Url:  pointer.To(v["url"].(string)),
	}
}
